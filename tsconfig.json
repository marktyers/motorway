{
  "compilerOptions": {
    "target": "es2022",
    "module": "ES2022",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true,
    "outDir": "./tmp",
    "rootDir": "src",
		"moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "build",
    "tmp"
  ]
}
